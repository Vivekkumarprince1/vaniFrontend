# Development URL - uncomment for local development
# VITE_API_URL=http://localhost:2000

# Production URL - used in deployed environments
VITE_API_URL=https://vani.azurewebsites.net

# Socket.IO Configuration
VITE_SOCKET_TIMEOUT=60000
VITE_SOCKET_RECONNECTION_ATTEMPTS=10
VITE_SOCKET_RECONNECTION_DELAY=2000

# Feature Flags
VITE_ENABLE_AUDIO_PROCESSING=true
VITE_ENABLE_VIDEO_CALLS=true
VITE_ENABLE_WEBSOCKET_FALLBACK=true
